{
  "name": "@web3auth/torus-wallet-connector-plugin",
  "version": "7.0.0-beta.0",
  "description": "Torus wallet connector for which can be used with Web3Auth or private key providers used inside Web3Auth",
  "keywords": [
    "web3Auth/torus-wallet-connector-plugin",
    "web3Auth",
    "multichainWallet",
    "blockchain",
    "ethereum",
    "metamask"
  ],
  "main": "dist/torusWalletConnectorPlugin.cjs.js",
  "module": "dist/torusWalletConnectorPlugin.esm.js",
  "unpkg": "dist/torusWalletConnectorPlugin.umd.min.js",
  "jsdelivr": "dist/torusWalletConnectorPlugin.umd.min.js",
  "types": "dist/types/index.d.ts",
  "author": "Torus Labs",
  "homepage": "https://github.com/Web3Auth/Web3Auth/tree/master/packages/plugins/torus-wallet-connector-plugin#readme",
  "license": "ISC",
  "scripts": {
    "test": "mocha --config ../../../.mocharc.json test/**.ts",
    "test-debugger": "mocha --config ../../../.mocharc.json --inspect-brk test/**.ts",
    "dev": "torus-scripts start",
    "build": "torus-scripts build",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepack": "npm run build",
    "pre-commit": "lint-staged --cwd ."
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@metamask/rpc-errors": "^6.0.0"
  },
  "dependencies": {
    "@toruslabs/torus-embed": "^3.0.0",
    "@web3auth/base": "^7.0.0-beta.0",
    "@web3auth/base-plugin": "^7.0.0-beta.0",
    "@web3auth/no-modal": "^7.0.0-beta.0",
    "loglevel": "^1.8.1"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.x"
  },
  "lint-staged": {
    "!(*d).ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Web3Auth/Web3Auth.git"
  },
  "bugs": {
    "url": "https://github.com/Web3Auth/Web3Auth/issues"
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  }
}
