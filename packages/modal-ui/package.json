    "web3Auth",
    "multichainWallet",
    "blockchain",
    "ethereum",
    "solana"
  ],
  "main": "dist/lib.cjs/packages/modal-ui/src/index.js",
  "module": "dist/lib.esm/packages/modal-ui/src/index.js",
